  "funding": "https://github.com/shannonhochkins/weather-icons-animated?sponsor=1",
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=18.0.0"
  },
  "main": "./dist/react-weather-icons.cjs.cjs",
